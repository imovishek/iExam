;\nconst loadUser = (dispatch) => {\n  const user = jwt.decode(localStorage.token);\n  const tabKey = localStorage.tabKey || \"dashboard\";\n  dispatch(setUserAction(user));\n  dispatch(setNavigaitonTabAction(tabKey));\n};\n\nconst App = ({ user, dispatch }) => {\n  const history = useHistory();\n  if (!user || !user.credential) {\n    if (!localStorage.token) return <Login />;\n    else loadUser(dispatch);\n    return <div></div>;\n  }\n  console.log(user);\n  let userType = \"\";\n  if (user.credential) userType = user.credential.userType;\n  return (\n    <div>\n      <BrowserRouter>\n        <Switch>\n          <Route path=\"/login\" component={Login} />\n          <Route path=\"/logout\" component={Logout} />\n          <Route path=\"/dashboard\" component={Dashboard} />\n          { hasPageAccess[userType] && hasPageAccess[userType].Courses && <Route path=\"/courses\" component={Courses} /> }\n          { hasPageAccess[userType] && hasPageAccess[userType].Teachers && <Route path=\"/teachers\" component={Teachers} /> }\n          <Route path=\"/\" component={Dashboard} />\n        </Switch>\n      </BrowserRouter>\n    </div>\n  );\n}\n\nconst mapStateToProps = state => ({\n  user: state.login.user\n});\n\nconst mapDispatchToProps = dispatch => ({\n  dispatch\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","/mnt/644040c9-7cd0-44b8-b2c2-906837be0658/4-1-Project/our-project/emsfrontend/src/reportWebVitals.js",[],["129","130"],"/mnt/644040c9-7cd0-44b8-b2c2-906837be0658/4-1-Project/our-project/emsfrontend/src/components/Dashboard/Dashboard.js",[],["131","132"],"/mnt/644040c9-7cd0-44b8-b2c2-906837be0658/4-1-Project/our-project/emsfrontend/src/components/Login/Login.js",["133","134","135"],"/mnt/644040c9-7cd0-44b8-b2c2-906837be0658/4-1-Project/our-project/emsfrontend/src/utitlities/api.js",[],["136","137"],"/mnt/644040c9-7cd0-44b8-b2c2-906837be0658/4-1-Project/our-project/emsfrontend/src/components/Logout/Logout.js",["138","139"],"import 